@page
@model MoviesForEveryone.Pages.MovieQueuePageModel
@{

}

<body id="background">
  <table style="width:auto">
        <tr>
            <th>Title</th>
            <th>Genres</th>
            <th>Keywords</th>
            <th>Director</th>
            <th>Description</th>
        </tr>
        @{   foreach (var movie in MovieQueuePageModel.movieQueue)
            {
                <tr>
                    <th>@movie.movieTitle</th>    
                    <th>@{ foreach (var genre in movie.genres)
                       {
                           @genre
                       }
                         }
                    </th>
                </tr>
            }
         }
        
    </table>   
</body>